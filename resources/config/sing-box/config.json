{
  "log": {
    "disabled": false,
    "level": "info",
    "output": "",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "cloudflare",
        "address": "1.1.1.1",
        "detour": "select"
      }
    ],
    "final": "cloudflare",
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 10800,
      "domain_strategy": "prefer_ipv4",
      "set_system_proxy": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct-out"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "selector",
      "tag": "select",

      "outbounds": []
    }
  ],
  "route": {
    "rules": [
      {
        "network": "tcp",
        "outbound": "select"
      },
      {
        "network": "udp",
        "outbound": "select"
      }
    ],
    "geoip": {
      "path": "./geoip.db",
      "download_detour": "direct-out"
    },
    "geosite": {
      "path": "./geosite.db",
      "download_detour": "direct-out"
    },
    "final": "select"
  }
}
